{
  "name": "springroll-kidaptive-plugin",
  "version": "0.1.8",
  "description": "Springroll plugin for Kidaptive ALP",
  "main": "./dist/kidaptive_alp_plugin.js",
  "scripts": {
    "install": "bower install",
    "build:debug": "cp src/js/main/KidaptiveAlpPlugin.js dist/kidaptive_alp_plugin.js",
    "build:prod": "uglifyjs src/js/main/KidaptiveAlpPlugin.js > dist/kidaptive_alp_plugin.min.js",
    "build": "npm run build:debug; npm run build:prod",
    "version": "npm run build && git add -A",
    "postversion": "git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Kidapt/springroll-kidaptive-plugin.git"
  },
  "keywords": [
    "springroll",
    "kidaptive",
    "alp",
    "plugin"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kidapt/springroll-kidaptive-plugin/issues"
  },
  "homepage": "https://github.com/Kidapt/springroll-kidaptive-plugin#readme",
  "devDependencies": {
    "uglify-js": "^3.0.14"
  }
}
